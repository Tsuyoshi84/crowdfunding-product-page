{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "content-security-policy",
          "value": "default-src 'none'; img-src 'self'; script-src 'self' 'unsafe-eval'; font-src fonts.gstatic.com; style-src 'self' fonts.googleapis.com"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
