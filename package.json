  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview --port 5173",
    "start": "vite --port 5173",
    "lint": "eslint './**/*.{ts,js,vue}'",
    "lint:fix": "eslint './**/*.{ts,js,vue}' --fix",
    "lint-css": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore",
    "lint-css:fix": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore --fix",
    "format": "prettier --write './**/*.{ts,js,vue}'",
    "format:check": "prettier --check './**/*.{ts,js,vue}'",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "cy": "cypress open",
    "cy:ct": "cypress open --component"
  },
  "dependencies": {
    "pinia": "^2.0.22",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.1.1",
    "@cypress/vue": "^4.2.0",
    "@types/node": "^17.0.45",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "cssnano": "5.1.13",
    "cypress": "^10.8.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.5.1",
    "local-cypress": "^1.2.6",
    "postcss-html": "^1.5.0",
    "postcss-nesting": "^10.2.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.12.1",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vue-template-compiler": "^2.7.10",
    "vue-tsc": "^0.40.13"
  }
}
