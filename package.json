{
	"name": "crowdfunding-product-page",
	"version": "0.0.0",
	"packageManager": "pnpm@10.2.1",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview --port 5173",
		"start": "vite --port 5173",
		"lint": "eslint './**/*.{ts,js,vue}'",
		"lint:biome": "biome check --apply ./src",
		"lint:fix": "eslint './**/*.{ts,js,vue}' --fix",
		"lint-css": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore",
		"lint-css:fix": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore --fix",
		"format": "biome format --write src",
		"format:check": "prettier --check './**/*.{ts,js,vue,css}'",
		"typecheck": "vue-tsc --noEmit --skipLibCheck",
		"cy": "cypress open",
		"cy:ct": "cypress open --component"
	},
	"dependencies": {
		"pinia": "^3.0.2",
		"vue": "^3.5.10"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@cypress/vite-dev-server": "^6.0.2",
		"@cypress/vue": "^6.0.2",
		"@types/node": "^22.14.1",
		"@typescript-eslint/parser": "^8.30.1",
		"@vitejs/plugin-vue": "^5.2.1",
		"cssnano": "^7.0.6",
		"cypress": "^14.3.0",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-vue": "^10.0.0",
		"local-cypress": "^1.2.6",
		"postcss-html": "^1.8.0",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^3.5.3",
		"stylelint": "^16.18.0",
		"stylelint-config-pretty-order": "^0.7.0",
		"stylelint-config-recommended-vue": "^1.6.0",
		"stylelint-config-standard": "^38.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-prettier": "^5.0.3",
		"stylelint-use-logical-spec": "^5.0.1",
		"typescript": "^5.7.3",
		"unplugin-auto-import": "^19.0.0",
		"unplugin-vue-components": "^28.0.0",
		"vite": "^6.0.15",
		"vue-template-compiler": "^2.7.16",
		"vue-tsc": "^2.2.8"
	}
}
