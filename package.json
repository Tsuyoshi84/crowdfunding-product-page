{
	"name": "crowdfunding-product-page",
	"version": "0.0.0",
	"license": "UNLICENSED",
	"engines": {
		"pnpm": "8",
		"yarn": "please-use-pnpm",
		"node": "^18.0.0"
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview --port 5173",
		"start": "vite --port 5173",
		"lint": "eslint './**/*.{ts,js,vue}'",
		"lint:fix": "eslint './**/*.{ts,js,vue}' --fix",
		"lint-css": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore",
		"lint-css:fix": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore --fix",
		"format": "prettier --write './**/*.{ts,js,vue,css}'",
		"format:check": "prettier --check './**/*.{ts,js,vue,css}'",
		"typecheck": "vue-tsc --noEmit --skipLibCheck",
		"cy": "cypress open",
		"cy:ct": "cypress open --component"
	},
	"dependencies": {
		"pinia": "^2.1.7",
		"vue": "^3.3.9"
	},
	"devDependencies": {
		"@cypress/vite-dev-server": "^5.0.6",
		"@cypress/vue": "^6.0.0",
		"@types/node": "^20.8.10",
		"@typescript-eslint/parser": "^6.7.5",
		"@vitejs/plugin-vue": "^4.4.0",
		"cssnano": "^6.0.1",
		"cypress": "^13.6.0",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-vue": "^9.17.0",
		"local-cypress": "^1.2.6",
		"postcss-html": "^1.5.0",
		"postcss-nesting": "^12.0.1",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^3.0.3",
		"stylelint": "^15.10.3",
		"stylelint-config-pretty-order": "^0.4.1",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^4.0.2",
		"stylelint-use-logical-spec": "^5.0.0",
		"typescript": "^5.2.2",
		"unplugin-auto-import": "^0.17.1",
		"unplugin-vue-components": "^0.25.2",
		"vite": "^4.5.1",
		"vue-template-compiler": "^2.7.15",
		"vue-tsc": "^1.8.24"
	}
}
