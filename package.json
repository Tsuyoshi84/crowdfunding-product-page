{
	"name": "crowdfunding-product-page",
	"version": "0.0.0",
	"packageManager": "pnpm@9.14.2",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview --port 5173",
		"start": "vite --port 5173",
		"lint": "eslint './**/*.{ts,js,vue}'",
		"lint:biome": "biome check --apply ./src",
		"lint:fix": "eslint './**/*.{ts,js,vue}' --fix",
		"lint-css": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore",
		"lint-css:fix": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore --fix",
		"format": "biome format --write src",
		"format:check": "prettier --check './**/*.{ts,js,vue,css}'",
		"typecheck": "vue-tsc --noEmit --skipLibCheck",
		"cy": "cypress open",
		"cy:ct": "cypress open --component"
	},
	"dependencies": {
		"pinia": "^2.2.3",
		"vue": "^3.5.10"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@cypress/vite-dev-server": "^5.2.1",
		"@cypress/vue": "^6.0.1",
		"@types/node": "^22.7.4",
		"@typescript-eslint/parser": "^8.12.2",
		"@vitejs/plugin-vue": "^5.1.4",
		"cssnano": "^7.0.6",
		"cypress": "^13.15.0",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-vue": "^9.30.0",
		"local-cypress": "^1.2.6",
		"postcss-html": "^1.7.0",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^3.3.3",
		"stylelint": "^16.10.0",
		"stylelint-config-pretty-order": "^0.7.0",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-order": "^6.0.4",
		"stylelint-prettier": "^5.0.2",
		"stylelint-use-logical-spec": "^5.0.1",
		"typescript": "^5.6.2",
		"unplugin-auto-import": "^0.18.3",
		"unplugin-vue-components": "^0.27.4",
		"vite": "^5.4.10",
		"vue-template-compiler": "^2.7.16",
		"vue-tsc": "^2.1.6"
	}
}
