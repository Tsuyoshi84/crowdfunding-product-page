{
	"name": "crowdfunding-product-page",
	"version": "0.0.0",
	"license": "UNLICENSED",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview --port 5173",
		"start": "vite --port 5173",
		"lint": "eslint './**/*.{ts,js,vue}'",
		"lint:fix": "eslint './**/*.{ts,js,vue}' --fix",
		"lint-css": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore",
		"lint-css:fix": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore --fix",
		"format": "prettier --write './**/*.{ts,js,vue,css}'",
		"format:check": "prettier --check './**/*.{ts,js,vue,css}'",
		"typecheck": "vue-tsc --noEmit --skipLibCheck",
		"cy": "cypress open",
		"cy:ct": "cypress open --component"
	},
	"dependencies": {
		"pinia": "^2.1.3",
		"vue": "^3.3.4"
	},
	"devDependencies": {
		"@cypress/vite-dev-server": "^5.0.5",
		"@cypress/vue": "^5.0.5",
		"@types/node": "^20.3.0",
		"@typescript-eslint/parser": "^5.59.8",
		"@vitejs/plugin-vue": "^3.2.0",
		"cssnano": "^6.0.1",
		"cypress": "^12.13.0",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-vue": "^9.14.1",
		"local-cypress": "^1.2.6",
		"postcss-html": "^1.5.0",
		"postcss-nesting": "^11.2.2",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^2.8.8",
		"stylelint": "^14.16.1",
		"stylelint-config-idiomatic-order": "^9.0.0",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^29.0.0",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^3.0.0",
		"stylelint-use-logical-spec": "^5.0.0",
		"typescript": "^5.1.3",
		"unplugin-auto-import": "^0.16.4",
		"unplugin-vue-components": "^0.25.0",
		"vite": "^3.2.4",
		"vue-template-compiler": "^2.7.14",
		"vue-tsc": "^1.6.5"
	}
}
