{
	"name": "crowdfunding-product-page",
	"version": "0.0.0",
	"packageManager": "pnpm@10.10.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview --port 5173",
		"start": "vite --port 5173",
		"lint": "eslint './**/*.{ts,js,vue}'",
		"lint:fix": "eslint './**/*.{ts,js,vue}' --fix",
		"lint-css": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore",
		"lint-css:fix": "stylelint '**/*.vue' '**/*.css' --ignore-path .gitignore --fix",
		"check": "biome check --write",
		"typecheck": "vue-tsc --noEmit --skipLibCheck",
		"cy": "cypress open",
		"cy:ct": "cypress open --component"
	},
	"dependencies": {
		"pinia": "^3.0.2",
		"vue": "^3.5.22"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@cypress/vite-dev-server": "^6.0.3",
		"@cypress/vue": "^6.0.2",
		"@types/node": "^24.0.15",
		"@typescript-eslint/parser": "^8.33.0",
		"@vitejs/plugin-vue": "^6.0.0",
		"cssnano": "^7.0.6",
		"cypress": "^14.3.2",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-vue": "^10.1.0",
		"local-cypress": "^1.2.6",
		"postcss-html": "^1.8.0",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^3.5.3",
		"stylelint": "^16.19.1",
		"stylelint-config-pretty-order": "^0.7.0",
		"stylelint-config-recommended-vue": "^1.6.0",
		"stylelint-config-standard": "^38.0.0",
		"stylelint-order": "^7.0.0",
		"stylelint-prettier": "^5.0.3",
		"stylelint-use-logical-spec": "^5.0.1",
		"typescript": "^5.8.3",
		"unplugin-auto-import": "^19.1.2",
		"unplugin-vue-components": "^28.5.0",
		"vite": "^7.0.7",
		"vue-template-compiler": "^2.7.16",
		"vue-tsc": "^3.0.3"
	}
}
